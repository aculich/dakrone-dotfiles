#!/usr/bin/env zsh

if [[ $QUEUENAME = '' ]]; then
    export QUEUENAME=`aws sqs list-queues | fgrep notifications | tr -d ' ' | tr -d "\""`
fi
# echo "Notifying to $QUEUENAME"
aws sqs send-message --queue-url $QUEUENAME --message-body \
    "{:queue \"queue.notifications\" :msg \"$*\"}" > /dev/null
